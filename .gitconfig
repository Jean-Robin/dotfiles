[user]
	name = Jean-Philippe CARUANA
	email = jp@barreverte.fr
[color]
	ui = auto
	interactive = auto
[core]
	editor = vim
	whitespace = -trailing-space
[diff]
	mnemonicPrefix = true
	wordRegex = .
[grep]
	extendedRegexp = true
[log]
	abbrevCommit = true
[merge]
	conflictStyle = diff3
[mergetool]
	keepBackup = false
	keepTemporaries = false
	prompt = false
[status]
	submoduleSummary = true
[alias]
	last = cat-file commit HEAD
	cm = commit -am\"[refactor] code mort\"
	duplication = commit -am\"[refactor] code mort : duplication\"
	format = commit -am\"[refactor] format\"
	rename = commit -am\"[refactor] rename\"
	st = status --short
	co = checkout
	br = branch -a --verbose
	rb = rebase
	rbi = rebase --interactive
	topo-log = log --oneline --graph --all --decorate
	spull = !git-svn fetch && git-svn rebase
	spush = !git-svn dcommit
	k = !gitk
	update-remote = !git fetch upstream && git merge upstream/trunk
	oops = commit --amend --all -C HEAD
	unstage = reset HEAD
	uncommit = reset --soft HEAD^
